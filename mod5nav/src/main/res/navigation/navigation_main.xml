<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation_main"
    app:startDestination="@id/buttonFragment3">
    <fragment
        android:id="@+id/buttonFragment3"
        android:name="com.example.mod5nav.ButtonFragment"
        android:label="fragment_button"
        tools:layout="@layout/fragment_button" >
        <action
            android:id="@+id/actionButtonToSecond"
            app:destination="@id/secondFragment3" />
    </fragment>
    <fragment
        android:id="@+id/secondFragment3"
        android:name="com.example.mod5nav.SecondFragment"
        android:label="fragment_second"
        tools:layout="@layout/fragment_second" >
        <argument
            android:name="user"
            app:argType="com.example.mod5nav.User" />
    </fragment>
</navigation>